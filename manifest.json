{
  "manifest_version": 3,
  "name": "Data Helper",
  "description": "Yazılım test süreçleri için hızlı ve güvenli fake data üretici",
  "version": "1.0.0",
  "minimum_chrome_version": "88",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Data Helper"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "storage",
    "contextMenus"
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/content-script-CKo9pKgP.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/content-script-CKo9pKgP.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
